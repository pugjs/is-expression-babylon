{
  "name": "is-expression-babylon",
  "version": "1.0.0",
  "description": "Check if a string is a valid JavaScript expression using Babylon",
  "keywords": [
    "javascript",
    "expression",
    "babylon"
  ],
  "dependencies": {
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.16.0",
    "babylon": "^6.9.1"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-istanbul": "^2.0.1",
    "babel-preset-es2015": "^6.14.0",
    "babel-register": "^6.16.3",
    "cross-env": "2.0.1",
    "nyc": "*",
    "testit": "^2.0.2"
  },
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "prepublish": "babel -d lib src",
    "test": "babel-node test && npm run coverage",
    "coverage": "cross-env NODE_ENV=test nyc --reporter text-summary --reporter lcov node test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pugjs/is-expression-babylon.git"
  },
  "author": "Timothy Gu <timothygu99@gmail.com>",
  "license": "MIT",
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "add-module-exports"
    ],
    "env": {
      "test": {
        "plugins": [
          "istanbul"
        ]
      }
    }
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  }
}
